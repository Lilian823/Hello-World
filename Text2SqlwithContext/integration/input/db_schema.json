{
"medical": [
      {
        "table_name": "patients",
        "description": "患者基本信息表",
        "columns": [
          {"name": "patient_id", "type": "VARCHAR(20)", "primary_key": true, "description": "患者唯一ID"},
          {"name": "name", "type": "VARCHAR(50)", "description": "患者姓名"},
          {"name": "gender", "type": "ENUM('男','女')", "description": "性别"},
          {"name": "birth_date", "type": "DATE", "description": "出生日期"},
          {"name": "phone", "type": "VARCHAR(20)", "description": "联系电话"},
          {"name": "address", "type": "VARCHAR(200)", "description": "联系地址"},
          {"name": "blood_type", "type": "ENUM('A','B','AB','O','未知')", "description": "血型"},
          {"name": "rh_factor", "type": "ENUM('阳性','阴性','未知')", "description": "Rh因子"},
          {"name": "allergy_history", "type": "TEXT", "description": "过敏史"},
          {"name": "chronic_diseases", "type": "TEXT", "description": "慢性病史"},
          {"name": "family_disease_history", "type": "TEXT", "description": "家族病史"},
          {"name": "created_at", "type": "TIMESTAMP", "description": "创建时间"}
        ]
      },
      {
        "table_name": "medical_checkup",
        "description": "体检记录表",
        "columns": [
          {"name": "id", "type": "INT", "primary_key": true, "auto_increment": true, "description": "记录ID"},
          {"name": "patient_id", "type": "VARCHAR(20)", "foreign_key": {"table": "patients", "column": "patient_id"}, "description": "患者ID"},
          {"name": "patient_name", "type": "VARCHAR(50)", "description": "患者姓名"},
          {"name": "gender", "type": "ENUM('男','女')", "description": "性别"},
          {"name": "age", "type": "INT", "description": "年龄"},
          {"name": "checkup_date", "type": "DATE", "description": "体检日期"},
          {"name": "height", "type": "DECIMAL(5,2)", "description": "身高(cm)"},
          {"name": "weight", "type": "DECIMAL(5,2)", "description": "体重(kg)"},
          {"name": "bmi", "type": "DECIMAL(5,2)", "description": "体质指数"},
          {"name": "blood_pressure", "type": "VARCHAR(10)", "description": "血压(mmHg)"},
          {"name": "fasting_glucose", "type": "DECIMAL(5,2)", "description": "空腹血糖(mmol/L)"},
          {"name": "total_cholesterol", "type": "DECIMAL(5,2)", "description": "总胆固醇(mmol/L)"},
          {"name": "triglycerides", "type": "DECIMAL(5,2)", "description": "甘油三酯(mmol/L)"},
          {"name": "hdl", "type": "DECIMAL(5,2)", "description": "高密度脂蛋白(mmol/L)"},
          {"name": "ldl", "type": "DECIMAL(5,2)", "description": "低密度脂蛋白(mmol/L)"},
          {"name": "alt", "type": "INT", "description": "谷丙转氨酶(U/L)"},
          {"name": "ast", "type": "INT", "description": "谷草转氨酶(U/L)"},
          {"name": "wbc", "type": "DECIMAL(5,2)", "description": "白细胞计数(×10^9/L)"},
          {"name": "rbc", "type": "DECIMAL(5,2)", "description": "红细胞计数(×10^12/L)"},
          {"name": "hemoglobin", "type": "DECIMAL(5,2)", "description": "血红蛋白(g/L)"},
          {"name": "urine_protein", "type": "ENUM('阴性','+-','+','++','+++','++++')", "description": "尿蛋白"},
          {"name": "ecg_result", "type": "VARCHAR(100)", "description": "心电图结果"},
          {"name": "ultrasound_result", "type": "VARCHAR(200)", "description": "超声检查结果"},
          {"name": "doctor_advice", "type": "TEXT", "description": "医生建议"},
          {"name": "created_at", "type": "TIMESTAMP", "description": "创建时间"}
        ]
      },
      {
        "table_name": "blood_pressure_records",
        "description": "血压记录表",
        "columns": [
          {"name": "id", "type": "INT", "primary_key": true, "auto_increment": true, "description": "记录ID"},
          {"name": "patient_id", "type": "VARCHAR(20)", "foreign_key": {"table": "patients", "column": "patient_id"}, "description": "患者ID"},
          {"name": "record_date", "type": "DATE", "description": "记录日期"},
          {"name": "record_time", "type": "TIME", "description": "记录时间"},
          {"name": "systolic", "type": "INT", "description": "收缩压(mmHg)"},
          {"name": "diastolic", "type": "INT", "description": "舒张压(mmHg)"},
          {"name": "pulse_rate", "type": "INT", "description": "脉搏(次/分)"},
          {"name": "measurement_condition", "type": "ENUM('静息','活动后','服药后','其他')", "description": "测量条件"},
          {"name": "notes", "type": "VARCHAR(200)", "description": "备注"}
        ]
      },
      {
        "table_name": "blood_glucose_records",
        "description": "血糖记录表",
        "columns": [
          {"name": "id", "type": "INT", "primary_key": true, "auto_increment": true, "description": "记录ID"},
          {"name": "patient_id", "type": "VARCHAR(20)", "foreign_key": {"table": "patients", "column": "patient_id"}, "description": "患者ID"},
          {"name": "record_date", "type": "DATE", "description": "记录日期"},
          {"name": "record_time", "type": "TIME", "description": "记录时间"},
          {"name": "glucose_value", "type": "DECIMAL(5,2)", "description": "血糖值(mmol/L)"},
          {"name": "measurement_type", "type": "ENUM('空腹','餐后2小时','随机','睡前','其他')", "description": "测量类型"},
          {"name": "notes", "type": "VARCHAR(200)", "description": "备注"}
        ]
      },
      {
        "table_name": "lipid_profile_records",
        "description": "血脂记录表",
        "columns": [
          {"name": "id", "type": "INT", "primary_key": true, "auto_increment": true, "description": "记录ID"},
          {"name": "patient_id", "type": "VARCHAR(20)", "foreign_key": {"table": "patients", "column": "patient_id"}, "description": "患者ID"},
          {"name": "test_date", "type": "DATE", "description": "检测日期"},
          {"name": "total_cholesterol", "type": "DECIMAL(5,2)", "description": "总胆固醇(mmol/L)"},
          {"name": "triglycerides", "type": "DECIMAL(5,2)", "description": "甘油三酯(mmol/L)"},
          {"name": "hdl", "type": "DECIMAL(5,2)", "description": "高密度脂蛋白(mmol/L)"},
          {"name": "ldl", "type": "DECIMAL(5,2)", "description": "低密度脂蛋白(mmol/L)"},
          {"name": "vldl", "type": "DECIMAL(5,2)", "description": "极低密度脂蛋白(mmol/L)"},
          {"name": "non_hdl_cholesterol", "type": "DECIMAL(5,2)", "description": "非高密度脂蛋白胆固醇(mmol/L)"},
          {"name": "notes", "type": "VARCHAR(200)", "description": "备注"}
        ]
      }
    ],
    "relationships": [
      {
        "from_table": "medical_checkup",
        "from_column": "patient_id",
        "to_table": "patients",
        "to_column": "patient_id",
        "relationship_type": "one-to-many"
      },
      {
        "from_table": "blood_pressure_records",
        "from_column": "patient_id",
        "to_table": "patients",
        "to_column": "patient_id",
        "relationship_type": "one-to-many"
      },
      {
        "from_table": "blood_glucose_records",
        "from_column": "patient_id",
        "to_table": "patients",
        "to_column": "patient_id",
        "relationship_type": "one-to-many"
      },
      {
        "from_table": "lipid_profile_records",
        "from_column": "patient_id",
        "to_table": "patients",
        "to_column": "patient_id",
        "relationship_type": "one-to-many"
      }
    ],
